{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "mon-projet": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/mon-projet",
            "index": "src/index.html",
            "browser": "src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
             
              "@angular/material/prebuilt-themes/indigo-pink.css",
              "src/assets/vendor/bootstrap/css/bootstrap.min.css",
              "src/assets/vendor/bootstrap-icons/bootstrap-icons.css",
              "src/assets/vendor/boxicons/css/boxicons.min.css",
              "src/assets/vendor/quill/quill.snow.css",
              "src/assets/vendor/quill/quill.bubble.css",
              "src/assets/vendor/remixicon/remixicon.css",
              "src/assets/vendor/simple-datatables/style.css",
              "src/assets/css/style.css",
              "src/assets/vendor/bootstrap/css/bootstrap.min.css",
              "src/assets/vendor/bootstrap-icons/bootstrap-icons.css",
              "src/assets/vendor/boxicons/css/boxicons.min.css",
              "src/assets/vendor/quill/quill.snow.css",
              "src/assets/vendor/quill/quill.bubble.css",
              "src/assets/vendor/remixicon/remixicon.css",
              "src/assets/vendor/simple-datatables/style.css",
              "src/assets/css/style.css",
              "src/assets/a/vendor/animate.css/animate.min.css" ,
              "src/assets/a/vendor/bootstrap/css/bootstrap.min.css" ,
              "src/assets/a/vendor/bootstrap-icons/bootstrap-icons.css" ,
              "src/assets/a/vendor/boxicons/css/boxicons.min.css",
              "src/assets/a/vendor/glightbox/css/glightbox.min.css" ,
              "src/assets/a/vendor/swiper/swiper-bundle.min.css" ,
              "src/assets/a/css/style.css"
             
              
            ],
            "scripts": [
            "src/assets/vendor/apexcharts/apexcharts.min.js",
            "src/assets/vendor/bootstrap/js/bootstrap.bundle.min.js",
            "src/assets/vendor/chart.js/chart.umd.js",
            "src/assets/vendor/echarts/echarts.min.js",
            "src/assets/vendor/quill/quill.min.js",
            "src/assets/vendor/simple-datatables/simple-datatables.js",
            "src/assets/vendor/tinymce/tinymce.min.js",
            "src/assets/vendor/php-email-form/validate.js",
            "src/assets/js/main.js",
            "src/assets/a/vendor/bootstrap/js/bootstrap.bundle.min.js",
            "src/assets/a/vendor/glightbox/js/glightbox.min.js",
            "src/assets/a/vendor/isotope-layout/isotope.pkgd.min.js",
            "src/assets/a/vendor/swiper/swiper-bundle.min.js",
            "src/assets/a/vendor/php-email-form/validate.js",
            "src/assets/a/js/main.js"
            
          ]

         
          

        
   

          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "mon-projet:build:production"
            },
            "development": {
              "buildTarget": "mon-projet:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "mon-projet:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
             
         
              "@angular/material/prebuilt-themes/indigo-pink.css",
              "src/assets/vendor/bootstrap/css/bootstrap.min.css",
              "src/assets/vendor/bootstrap-icons/bootstrap-icons.css",
              "src/assets/vendor/boxicons/css/boxicons.min.css",
              "src/assets/vendor/quill/quill.snow.css",
              "src/assets/vendor/quill/quill.bubble.css",
              "src/assets/vendor/remixicon/remixicon.css",
              "src/assets/vendor/simple-datatables/style.css",
              "src/assets/css/style.css",
              "src/assets/vendor/bootstrap/css/bootstrap.min.css",
              "src/assets/vendor/bootstrap-icons/bootstrap-icons.css",
              "src/assets/vendor/boxicons/css/boxicons.min.css",
              "src/assets/vendor/quill/quill.snow.css",
              "src/assets/vendor/quill/quill.bubble.css",
              "src/assets/vendor/remixicon/remixicon.css",
              "src/assets/vendor/simple-datatables/style.css",
              "src/assets/css/style.css",                          
              "src/assets/a/vendor/animate.css/animate.min.css" ,
              "src/assets/a/vendor/bootstrap/css/bootstrap.min.css" ,
              "src/assets/a/vendor/bootstrap-icons/bootstrap-icons.css" ,
              "src/assets/a/vendor/boxicons/css/boxicons.min.css",
              "src/assets/a/vendor/glightbox/css/glightbox.min.css" ,
              "src/assets/a/vendor/swiper/swiper-bundle.min.css" ,
              "src/assets/a/css/style.css"
              
            ],
            "scripts": [
            "src/assets/vendor/apexcharts/apexcharts.min.js",
            "src/assets/vendor/bootstrap/js/bootstrap.bundle.min.js",
            "src/assets/vendor/chart.js/chart.umd.js",
            "src/assets/vendor/echarts/echarts.min.js",
            "src/assets/vendor/quill/quill.min.js",
            "src/assets/vendor/simple-datatables/simple-datatables.js",
            "src/assets/vendor/tinymce/tinymce.min.js",
            "src/assets/vendor/php-email-form/validate.js",
            "src/assets/js/main.js",
            "src/assets/a/vendor/bootstrap/js/bootstrap.bundle.min.js",
            "src/assets/a/vendor/glightbox/js/glightbox.min.js",
            "src/assets/a/vendor/isotope-layout/isotope.pkgd.min.js",
            "src/assets/a/vendor/swiper/swiper-bundle.min.js",
            "src/assets/a/vendor/php-email-form/validate.js",
            "src/assets/a/js/main.js"
            
         
          ]
          }
        }
      }
    }
  }
}
